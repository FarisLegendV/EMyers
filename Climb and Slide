--Carga Faris Gui
local a=loadstring(game:HttpGet("https://raw.githubusercontent.com/liebertsx/Tora-Library/main/src/library",true))()
local b=a:CreateWindow("Farislegend")
local c=b:AddFolder("Farm")
c:AddButton({text="Get Best Pet",callback=function()
    local d={"Give_Pet","Pet_Honey_Mutant"}
    game:GetService("ReplicatedStorage"):WaitForChild("R_Pets"):FireServer(unpack(d))
end})
local e="Map_1"
c:AddList({
    text="Select Map",
    values={"Map_1","Map_2","Map_3"},
    callback=function(f)e=f end,
    flag="map_selector"
})
c:AddToggle({
    text="Auto Coins",
    flag="auto_slider_toggle",
    callback=function(g)
        _G.autoSlider=g
        while _G.autoSlider do
            local h=workspace:WaitForChild("Maps"):FindFirstChild(e)
            if h then
                local i={"Slide",h}
                pcall(function()
                    game:GetService("ReplicatedStorage"):WaitForChild("R_Server"):FireServer(unpack(i))
                end)
            end
            task.wait(0.5)
        end
    end
})
a:Init()
